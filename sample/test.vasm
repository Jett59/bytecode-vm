# Add all of the odd numbers less than 30000000

# Locals:
# 0: i
# 1: total

# Set i = total = 0
ipush 0
dup
lstore 0
lstore 1

goto check

loop:
lload 0
dup
irshift 1
ilshift 1
sub
ipush 1
cgoto_neq increment

odd:
lload 0
lload 1
add
lstore 1

increment:
lload 0
iadd 1
lstore 0

check:
lload 0
ipush 30000000
cgoto_lt loop

done:
lload 1
exit
